const32(x;a;b;c;d;e;f;g;h;temp0)
{
	!
	? x     0() a
	?       *10(x    ;temp0)
	? x     b

	? x >   0() c
	?       *10(x >  ;temp0)
	? x >   d

	? x >>  0() e
	?       *10(x >> ;temp0)
	? x >>  f

	? x >>> 0() g
	?       *10(x >>>;temp0)
	? x >>> h

	? ~
}


zero32(x)
{
	! x
	0() > 0() > 0() > 0()
	? ~
}


