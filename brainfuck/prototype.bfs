		stack5();stack5() >;stack5() >>;stack5() >>>;stack6();stack6() >)
	xor32(s1();s1();stack4();                    # s1 ^= (w[i-2] rightrotate 19)
		stack5();stack5() >;stack5() >>;stack5() >>>;stack6();stack6() >;stack6() >>;stack6() >>>)
	rightrotate32(stack4();stack3();stack1() >>>;#stack4 = (w[i-2] rightrotate 17)
		stack5();stack5() >;stack5() >>;stack5() >>>;stack6();stack6() >)
